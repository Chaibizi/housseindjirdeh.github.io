{
  "private": true,
  "name": "houssein.me",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run -s serve || npm run -s dev",
    "build": "preact build --template src/index.html",
    "serve": "preact build --template src/index.html && preact serve",
    "dev": "preact watch",
    "lint": "eslint src",
    "test": "jest ./tests"
  },
  "eslintConfig": {
    "extends": "eslint-config-synacor"
  },
  "eslintIgnore": [
    "build/*"
  ],
  "devDependencies": {
    "copy-webpack-plugin": "^4.5.1",
    "eslint": "^4.9.0",
    "eslint-config-synacor": "^2.0.2",
    "identity-obj-proxy": "^3.0.0",
    "if-env": "^1.0.0",
    "preact-cli": "^2.1.0",
    "preact-cli-workbox-plugin": "^1.0.0",
    "preact-render-spy": "^1.2.1"
  },
  "dependencies": {
    "highlight.js": "^9.12.0",
    "marked": "^0.3.5",
    "preact": "^8.2.6",
    "preact-markup": "^1.6.0",
    "preact-router": "^2.5.7",
    "yaml": "^0.3.0"
  }
}
